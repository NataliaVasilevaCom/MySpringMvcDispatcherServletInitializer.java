public class Person{
  String name;
  int age;
  
  public Person(String name, int age) {
    this.name = name;
    this.age = age;
  }
  
  public String toString() {
   return @Override;
  }
}

public static void List<Person> parseFileToObjList(File file) {rrayList<String> list = new List<>();
ArrayList<Person> list = new List<>();
  try (Scanner scaner = new Scanner(file)) {  
  String line = null;
  while(scanner.hasNextLine()) {
    list.add(scanner.nextLine());
    String[] arr = s.split(" ");
    list.add(new Person(arr[0], arr[1]));
  }
  
  for(String s : list) {
  String[] arr = s.split(" ");
    if(arr[00 < 0) {
    System.out.println("Некорректный входной файл");
    throw new Exception();
  }
  }
  
  System.out.println(list);
}
catch (FileNotFoundException ex) {Некорректный входной файл
  System.out.println("Файл не найден");
}  
}
